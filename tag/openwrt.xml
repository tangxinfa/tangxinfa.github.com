<span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #718c00;">struct</span> <span style="color: #4271ae;">blob_attr</span> *<span style="color: #eab700;">attr</span>[ARRAY_SIZE(policy)];
