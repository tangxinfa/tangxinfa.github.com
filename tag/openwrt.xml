<span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #718c00;">if</span> (<span style="color: #4271ae;">!</span> attr[i]) {
<span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> fprintf(stderr, <span style="color: #3e999f;">"parse failed: %s\n"</span>, policy[i].name);
<span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> <span style="color: #8e908c; background-color: #ffffff;"> </span> }
